<% if signed_in? %>
  <table>
    <tr>
      <td class="main">
        <h1 class="micropost">Whats up?</h1>
          <%= render "shared/micropost_form" %>
      </td>
    </tr>
  </table>
<% else %>
  <h1>Pages#home</h1>
  <p>Find me in app/views/pages/home.html.erb</p>

  <%= link_to "Sign up now!", signup_path, :class => "signin_button round" %>
<% end -%>